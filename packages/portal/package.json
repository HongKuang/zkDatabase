{
  "name": "zkdb",
  "version": "0.1.1",
  "description": "zkDatabase for the future of Web3",
  "author": "chiro@orochi.network",
  "license": "Apache-2.0",
  "keywords": [
    "zk",
    "database",
    "zk-database",
    "zkdb",
    "zkdatabase"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orochi-network/zkDatabase.git"
  },
  "bugs": {
    "url": "https://github.com/orochi-network/zkDatabase/issues"
  },
  "homepage": "https://github.com/orochi-network/zkDatabase#readme",
  "main": "./build/src/index.js",
  "types": "./build/src/index.d.ts",
  "scripts": {
    "release": "rm -rf ./build && npm run build && npm publish --access public",
    "build": "tsc -p tsconfig.json",
    "buildw": "tsc -p tsconfig.json --watch",
    "format": "prettier --write --ignore-unknown **/*",
    "prepare": "cd .. && cd .. && husky install packages/zkdb/.husky",
    "dev": "npx nodemon -e ts -w ./src -x \"npx ts-node ./src/index.ts\"",
    "knex:migrate": "knex migrate:latest",
    "lint": "npx eslint ./src/**/*.ts --fix",
    "lint-check": "npx eslint ./src/**/*.ts --debug"
  },
  "lint-staged": {
    "**/*": [
      "eslint src/* --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express-fileupload": "^1.4.1",
    "@types/node": "^20.4.10",
    "@types/safer-buffer": "^2.1.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-snarkyjs": "^0.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "^2.8.8",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.7",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@apollo/server": "^4.9.1",
    "@chainsafe/libp2p-noise": "^12.0.1",
    "@chainsafe/libp2p-yamux": "^4.0.1",
    "@helia/interface": "^1.0.0",
    "@helia/ipns": "^1.1.1",
    "@helia/unixfs": "^1.2.2",
    "@libp2p/delegated-peer-routing": "^4.0.9",
    "@libp2p/kad-dht": "^9.3.8",
    "@libp2p/peer-id": "^2.0.4",
    "@libp2p/tcp": "^7.0.1",
    "@libp2p/websockets": "^6.0.1",
    "@multiformats/multiaddr": "^12.1.2",
    "@orochi-network/framework": "^1.1.8",
    "@zkdb/kubo": "^1.0.5",
    "axios": "^1.5.0",
    "blockstore-fs": "^1.1.1",
    "bson": "^5.3.0",
    "cors": "^2.8.5",
    "datastore-fs": "^9.1.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "google-auth-library": "^9.0.0",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.22.2",
    "graphql-type-json": "^0.3.2",
    "helia": "^1.0.4",
    "joi": "^17.9.2",
    "jose": "^4.14.4",
    "knex": "^2.5.1",
    "kubo-rpc-client": "^3.0.1",
    "libp2p": "^0.45.9",
    "multiformats": "^11.0.2",
    "redis": "^4.6.7",
    "safer-buffer": "^2.1.2",
    "snarkyjs": "^0.11.4",
    "uuid": "^9.0.0"
  }
}
